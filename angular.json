{
  "version": 1,
  "projects": {
    "agent": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/agent",
      "sourceRoot": "apps/agent/src",
      "prefix": "fmg-agent",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["lodash", "minimatch"],
            "outputPath": "dist/apps/agent",
            "index": "apps/agent/src/index.html",
            "main": "apps/agent/src/main.ts",
            "polyfills": "apps/agent/src/polyfills.ts",
            "tsConfig": "apps/agent/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "libs/static/src/assets",
                "output": "assets"
              },
              {
                "glob": "favicon.ico",
                "input": "libs/static/src",
                "output": ""
              },
              {
                "glob": "web.config",
                "input": "libs/static/src",
                "output": ""
              }
            ],
            "styles": ["libs/style/src/index.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/style/src/lib/common"]
            },
            "scripts": []
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "libs/config/src/lib/environments/environment.ts",
                  "with": "libs/config/src/lib/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "libs/config/src/lib/environments/environment.ts",
                  "with": "libs/config/src/lib/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "agent:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "agent:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "agent:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/agent/tsconfig.app.json",
              "apps/agent/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/agent/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/agent/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "agent-e2e": {
      "root": "e2e/agent",
      "sourceRoot": "e2e/agent/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "e2e/agent/cypress.json",
            "tsConfig": "e2e/agent/tsconfig.e2e.json",
            "devServerTarget": "agent:serve"
          },
          "configurations": {
            "prod": {
              "devServerTarget": "agent:serve:prod"
            },
            "uat": {
              "devServerTarget": "agent:serve:uat"
            },
            "qa": {
              "devServerTarget": "agent:serve:qa"
            },
            "dev": {
              "devServerTarget": "agent:serve:dev"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/agent/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!e2e/agent/**/*"]
          }
        }
      }
    },
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "fmg-auth",
      "architect": {
        "lint": {
